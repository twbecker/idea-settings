<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Build Project">
        <Item>
          <IdeaActionID>CompileDirty</IdeaActionID>
          <ShortCut>'Ctrl+F9'</ShortCut>
          <Description>Build Project</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Class...">
        <Item>
          <IdeaActionID>GotoClass</IdeaActionID>
          <ShortCut>'Ctrl+N'</ShortCut>
          <Description>Class...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'ffs'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'ffs'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Recompile 'ExperimentStore.java'">
        <Item>
          <IdeaActionID>Compile</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F9'</ShortCut>
          <Description>Recompile 'ExperimentStore.java'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Run Individual Robot Test'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'Run Individual Robot Test'</Description>
          <Count>32</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Run Robot Test'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'Run Robot Test'</Description>
          <Count>17</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>11</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'testVariationGener...()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Run 'testVariationGener...()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Step Over</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Left' or 'Button4 Click'</ShortCut>
          <Description>Back</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
